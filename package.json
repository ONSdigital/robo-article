{
  "name": "@onsvisual/robo-article",
  "homepage": "http://onsvisual.github.io/robo-article",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev --mode preview",
    "build": "vite build && node ./scripts/js-fix && node ./scripts/gen-sitemap",
    "build:preview": "cross-env APP_ENV=preview vite build",
    "build:data": "node ./scripts/build-data",
    "package": "vite package",
    "preview": "vite preview",
    "deploy": "gh-pages -d build -t",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "author": "Ahmad Barclay",
  "license": "MIT",
  "devDependencies": {
    "@onsvisual/robo-utils": "^0.3.1",
    "@onsvisual/svelte-charts": "^0.4.11",
    "@onsvisual/svelte-components": "^1.0.5",
    "@sveltejs/adapter-static": "^3.0.0",
    "@sveltejs/kit": "^2.5.27",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "cross-env": "^7.0.3",
    "d3-dsv": "latest",
    "d3-format": "^3.1.0",
    "gh-pages": "^6.3.0",
    "html2canvas": "^1.4.1",
    "node-html-parser": "^6.1.5",
    "parse-color": "^1.0.0",
    "prettier": "^3.1.0",
    "prettier-plugin-svelte": "^3.2.6",
    "pug": "^3.0.2",
    "svelte": "^5.0.0",
    "vite": "^5.4.4"
  },
  "dependencies": {
    "sirv-cli": "^1.0.0"
  },
  "type": "module"
}
